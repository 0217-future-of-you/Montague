
page{
		background-color: white;
}
.u-f,.u-f-ac,.u-f-ajc{
	   display: -webkit-box;
	   display: -webkit-flex;
	   display: flex;
}
.u-f-ac, .u-f-ajc{
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.u-f-ajc{
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.swiper-tab-list{
		width: 376rpx;

		color: #969696;
		font-weight: bold;
}
.uni-tab-bar .active{
		color: #343434;
}
.active .swiper-tab-line{
		border-bottom: 6rpx solid #FEDE33; 
		width: 180rpx;
		margin: auto;
		border-top:6rpx solid #FEDE33;
}
